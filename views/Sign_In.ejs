<link rel="stylesheet" href="../assets/css/Sign_In.css">
<div id="Sign_In">
    <a href="/">HOME</a>
    <h1>
        This is Sign-In Page
    </h1>
    
    <form action="/users/create-session" method="POST">
        <input type="email" name="email" placeholder="Email"  required>
        <input type="password" name="password" placeholder="Password"  required>
        <input type="submit" value="Sign In">
    
    </form>
    <p><a href="/users/forgot-password-page">forgot password?</a></p>
    
    <p id="googleAuth">
        <a href="/users/auth/google" class="google-signin-btn">Google Sign In/Sign Up</a>
    </p>

</div>
